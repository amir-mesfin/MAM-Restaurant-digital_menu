# Node modules
node_modules/

# Environment variables
.env

# Log files
npm-debug.log*
yarn-debug.log*
yarn-error.log*

# Build output
dist/
build/

# Ignore coverage directory for testing
coverage/

# Ignore local database files
*.sqlite
*.db

# Ignore temporary files
*.tmp
*.temp

# Ignore IDE/editor-specific files
.idea/              # JetBrains IDEs
.vscode/            # Visual Studio Code settings
*.sublime-workspace # Sublime Text workspace files

# Ignore operating system files
.DS_Store          # macOS
Thumbs.db          # Windows
ehthumbs.db        # Windows
Desktop.ini        # Windows

# Ignore backup files
*~                  # Backup files created by some editors
*.bak               # Backup files with .bak extension

# Ignore npm lock files (if you are using Yarn)
package-lock.json   # npm
yarn.lock           # Yarn

# Ignore compiled files for languages
*.class             # Java class files
*.o                 # Object files
*.pyc               # Python bytecode
*.pdb               # Debugging symbols
